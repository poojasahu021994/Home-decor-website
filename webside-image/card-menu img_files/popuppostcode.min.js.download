define(['jquery','mage/cookies'],function($){$.widget('magedelight.popuppostcode',{options:{},_create:function(){$(document).on('submit','#form-product-postcode',function(event){event.preventDefault();callPopupAjax();});$(document).ready(function(){var postcodeCookieCheck=$.cookie('_postcode_info');if(postcodeCookieCheck){postcodeCookieCheck=$.parseJSON(postcodeCookieCheck);callOnLoadAjax(postcodeCookieCheck);}
if(!postcodeCookieCheck){callOnLoadAjax('560001');}});function callOnLoadAjax(value){$.cookie('_postcode_info',value,{expires:1});$('#pincodeBtn span').text(value);$('#postcode').val(value);}
